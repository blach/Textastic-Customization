			"scope": "text.tex.latex",
			"pattern": "(?<!\\\\)\\\\([a-zA-Z\\*]*)",
			"completionCaptureIndex": 1,
			"completionSetNames": [
				"latex.commands",
				"latex.commands.amsmath",
				"latex.commands.physics"
			]
		},
		{
			"description": "LaTeX Environment Completion",
			"scope": "text.tex.latex",
			"pattern": "(?<!\\\\)\\\\([a-zA-Z\\*]*)\\{(.*)",
			"completionCaptureIndex": 2,
			"completionSetNames": [
				"latex.commands.$1.environments",
				"latex.commands.$1.environments.amsmath"
			]
		}, 
		{
			"description": "LaTeX Package Completion",
			"scope": "text.tex.latex",
			"pattern": "(?<!\\\\)\\\\([a-zA-Z\\*]*)(?:\\[.*\\])?\\{(.*)",
			"completionCaptureIndex": 2,
			"completionSetNames": [
