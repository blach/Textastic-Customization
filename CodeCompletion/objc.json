	"completionSets": [
    	{
    		"name": "objc.keywords.exceptions",
    		"defaultAppend": "",

    		"strings": [
                { "string": "try", "append": " {\n\t$0\n}\n" },
                { "string": "try-catch", "replace": "try {\n\t${1:statements}\n} @catch (${2:NSException *exception}) {\n\t${3:handler}\n}\n" },
                { "string": "try-catch-finally", "replace": "try {\n\t${1:statements}\n} @catch (${2:NSException *exception}) {\n\t${3:handler}\n} @finally {\n\t${4:statements}\n}\n" },
                { "string": "catch", "append": " (${1:NSException *exception}) {\n\t${3:handler}\n}\n" },
                { "string": "finally", "append": " {\n\t$0\n}\n" },
                { "string": "throw", "append": " ${0:expression}" }
    		]
    	},
        {
            "name": "objc.keywords.allocation",
            "defaultAppend": "",

            "strings": [
                "alloc",
                "autorelease",
                "retain",
                "release",
                "assign",
                "strong",
                "weak",
                "unsafe_unretained"
            ]
        },
        {
            "name": "objc.keywords.dataTypes",
            "defaultAppend": "",

            "strings": [
                "BOOL"
            ]
        },
        {
            "name": "objc.constants",
            "defaultAppend": "",
            
            "strings": [
                "NO",
                "YES",
                "nil"
            ]
        },
        {
            "name": "objc.preprocessor.directives",
            "strings": [
                { "string": "import <", "append": "${1:filename}>\n" },
                { "string": "import \"", "append": "${1:filename}\"\n" }
            ]
        },
		{
			"name": "objc.keywords.class_definition",
			"defaultAppend": "\n",
            
			"strings": [
                { "string": "interface", "append": " ${1:ClassName}: ${2:NSObject} ${3:<Protocol>}\n\n$0\n\n@end\n" },

                "end",
                "implementation",

                "private",
                "protected",
                "public",
                "package"
            ]
        },
        {
            "name": "objc.keywords.protocol_definition",
            "defaultAppend": "",
            
            "strings": [
                { "string": "protocol", "append": " ${1:ProtocolName} ${2:<NSObject>}\n\n${0:methods}\n\n@end\n" },

                "required",
                "optional"
            ]
        },
        {
            "name": "objc.keywords.property_definition",
            "defaultAppend": "",
            
            "strings": [
                { "string": "property", "append": " (${1:nonatomic,} ${2:assign}) ${3:type} ${4:name};" },
                { "string": "synthesize", "append": " ${1:property};" },
                "dynamic"
            ]
        },
        {
            "name": "objc.keywords.other",
            "defaultAppend": "",
            
            "strings": [
                { "string": "class", "append": " $1;" },
                { "string": "encode", "append": "(${1:type_spec});" },
                { "string": "selector", "append": "(${1:arg})" },
                { "string": "autoreleasepool", "append": "{\n\t$0\n}"}
            ]
        },
